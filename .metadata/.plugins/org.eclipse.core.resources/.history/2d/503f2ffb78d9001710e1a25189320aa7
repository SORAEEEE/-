import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		// Scanner sc = new Scanner(System.in);
		// int N = sc.nextInt();
		int i = 1;
		int num; // ³ª¸ÓÁö

		for (int N = 1; N < 100; N++) {
			while (N / sum(i) != 0) {
				i++;
			}
			num = N % sum(i - 1);
			System.out.println(i + " " + num);
		}

		//sc.close();
	}

	static int sum(int i) {
		return (3 * i * (i - 1) + 2);
	}
}
