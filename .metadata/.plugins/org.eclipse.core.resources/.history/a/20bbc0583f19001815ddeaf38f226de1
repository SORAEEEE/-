import java.util.ArrayList;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		ArrayList<String>[] strArrList;
		int N = sc.nextInt();
		String temp;

		strArrList = (ArrayList<String>[]) new ArrayList[N+1];
		for (int i = 1; i <= N; i++) {
			strArrList[i] = new ArrayList<String>();
		}

		for (int i = 1; i <= N; i++) {
			temp = sc.next();
			for (int j = 0; j < temp.length(); j++) {
				strArrList[i].add(temp.substring(j, j + 1));
			}
		}
		for (int i = 1; i <= N; i++) {
			for (int j = 0; j < strArrList[i].size(); j++) {
				System.out.print(strArrList[i].get(j)+ " ");
			}
			System.out.println();
		}
		sc.close();
	}
}
