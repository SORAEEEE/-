import java.util.ArrayList;
import java.util.Scanner;

public class Main {
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		ArrayList<String>[] strArrList;
		int N = sc.nextInt();
		String temp;
		
		strArrList = (ArrayList<String>[]) new ArrayList[N];
		for(int i = 0; i<N ; i++){
			strArrList[i] = new ArrayList<String>();
		}
		
		for(int i = 0; i<N; i++){
			temp = sc.nextLine();
			for(int j = 0; j<temp.length(); j++){
				strArrList[i].add(temp.substring(j,j+1)); //?가 많으면 next()로
			}
		}
		
		for(int i =0;i<3; i++){
			System.out.println(strArrList[1].get(i));	
		}
		
		sc.close();
	}
}
