import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int X = sc.nextInt();
		int i = 1;

		while (X / sum(i) != 0) {
			i++;
		}
		
		System.out.println(i +" "+ (X%sum(i)));

	}

	static int sum(int i) {
		return i * (i + 1) / 2;
	}
}
