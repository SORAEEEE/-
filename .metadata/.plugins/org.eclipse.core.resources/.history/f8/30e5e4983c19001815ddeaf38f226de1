		Scanner sc = new Scanner(System.in);
		ArrayList<char[]> charArrayList = new ArrayList<char[]>();
		int N = sc.nextInt();
		String temp;
		
		for(int i = 0; i<N; i++){
			temp = sc.nextLine();
			for(int j = 0; j<temp.length(); j++){
				adj[i].add(temp.charAt(j)); //?가 많으면 next()로
			}
			
		}
		
