import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		// Scanner sc = new Scanner(System.in);
		// int X = sc.nextInt();
		for (int X = 1; X < 100; X++) {
			int i = 1;

			while (X / sum(i) != 0) {
				i++;
			}

			System.out.println(i + " " + (X - sum(i - 1)));
		}
	}

	static int sum(int i) {
		return i * (i + 1) / 2;
	}
}
