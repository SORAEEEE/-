import java.util.Scanner;

public class Main {
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int X = sc.nextInt();
		int i = 1;
		int sum = i*(i+1)/2;
		
		while(i<5){
			System.out.println(sum(i));
			i++;
		}
		
	}
	
	static int sum(int i){
		return i*(i+1)/2;
	}
}
